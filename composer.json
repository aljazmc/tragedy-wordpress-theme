{
    "name": "aljazmc/tragedy-wordpress-theme",
    "description": "The most tragic WordPress theme",
    "type": "wordpress-theme",
    "license": "GPL-2.0-or-later",
    "autoload": {
        "psr-4": {
            "Aljazmc\\TragedyWordpressTheme\\": "src/"
        }
    },
    "authors": [
        {
            "name": "aljazmc",
            "email": "mlinaric.aljaz@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
